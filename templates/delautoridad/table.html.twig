{% for tr in objects %}
    <tr>
        <td>{{ tr.nombre }}</td>
        <td>{{ tr.cargo }}</td>
        {% if tr.archivo not in [null, '', 'N/A'] %}
            <td><a href="#" onclick="window.open('{{ '/delautoridad' ~ tr.archivo }}')">{{ tr.archivo|slice(9) }}</a></td>
        {% else %}
            <td></td>
        {% endif %}
        {% if tr.foto not in [null, '', 'N/F'] %}
            {% if tr.foto %}
                <td><img src="{{ '/delautoridad' ~ tr.foto }}" style="height: 75px; width: auto;" alt="{{ tr.foto|slice('/foto/'|length) }}"></td>
            {% else %}
                <td>S/F</td>
            {% endif %}
        {% else %}
            <td>S/F</td>
        {% endif %}
        <td align="center">
        {% if 'delautoridad_editar' in permisos %}  
            <button id="edit" data-json="{{tr.id}}" type="button" class="btn bg-indigo waves-effect waves-light edit" title="Editar"><i class="material-icons">create</i></button>
        {% endif %}
        {% if 'delautoridad_eliminar' in permisos %}  
            <button id="delete" data-json="{{tr.id}}" type="button" class="btn bg-red waves-effect waves-light delete" title="Eliminar"><i class="material-icons">clear</i></button>
        {% endif %}
        </td>
    </tr>
{% endfor %}