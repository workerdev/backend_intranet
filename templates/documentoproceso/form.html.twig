<div id="form" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog dialog-own" role="document">

        <div class="modal-content">
            <form id="submit_data" class="new" enctype="multipart/form-data" novalidate>
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h3 id="cliente_tittle" class="modal-title">Documento en Proceso</h3>
                    <h4 id="cliente_enable" class=""></h4>
                </div>

                <div class="modal-body">
                    <div id="cliente_form_body" class="box-body">
                        <div id="id_div" class="form-group">
                            <div class="form-line">
                                <input id="id" name="id" type="text" class="form-control" disabled="disabled">
                                <label class="form-label">Doc. ID</label>
                            </div>
                        </div>
                        
                        <div class="row clearfix">
                            <div class="col-sm-6">
                                <div class="form-group form-float">
                                    <label>Nuevo/Actualización</label>
                                    <select id="nuevoactualizacion" name="nuevoactualizacion" class="form-control show-tick validate">
                                        <option value="NUEVO">NUEVO</option>
                                        <option value="ACTUALIZACION">ACTUALIZACION</option>
                                        <option value="BAJA">BAJA</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group form-float">
                                    <label>Tipo de documento</label>
                                    <select id="fktipo" name="fktipo" class="form-control show-tick validate">
                                        {% for tp in tipo %}
                                            <option value="{{ tp.id }}">{{ tp.nombre }}</option>
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>
                        </div>


                        <div class="form-group form-float">
                            <div class="form-line">
                                <input id="codigonuevo" name="codigonuevo" type="text" class="form-control">
                                <label for="codigonuevo" class="form-label">Código del documento</label>
                            </div>
                        </div>
                        <div class="form-group form-float">
                            <label>Cód. del documento</label>
                            <select id="fkdocumento" name="fkdocumento" class="form-control show-tick">
                                {% for d in documento %}
                                    <option value="{{ d.id }}">{{ d.codigo }}</option>
                                {% endfor %}
                            </select>
                            <br>
                            <br>
                        </div>
                        <div class="form-group form-float">
                            <label>Código doc. formulario</label>
                            <select id="fkformulario" name="fkformulario" class="form-control show-tick">
                                {% for f in formulario %}
                                    <option value="{{ f.id }}">{{ f.codigo }}</option>
                                {% endfor %}
                            </select>
                            <br>
                            <br>
                        </div>
                        <div class="form-group form-float">
                            <label class="form-label">Título</label>
                            <div class="form-line">
                                <textarea id="titulo" name="titulo" class="md-textarea form-control txta-own validate"></textarea>
                            </div>
                        </div>

                        <div class="row clearfix">
                            <div class="col-sm-6">
                                <div class="form-group form-float">
                                    <div class="form-line">
                                        <input id="versionvigente" name="versionvigente" type="text" class="form-control text validate">
                                        <label class="form-label">Versión vigente</label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group form-float">
                                    <div class="form-line">
                                        <input id="carpetaoperativa" name="carpetaoperativa" type="text" class="form-control text">
                                        <label class="form-label">Carpeta operativa</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-group form-float">
                            <label>Código del proceso</label>
                            <select id="fkproceso" name="fkproceso" class="form-control show-tick validate">
                                {% for p in proceso %}
                                    <option value="{{ p.id }}">{{ p.codproceso }}</option>
                                {% endfor %}
                            </select>
                            <br>
                            <br>
                        </div>
                        <div class="form-group form-float">
                            <div class="form-line">
                                <label class="form-label">Fecha</label><br>
                                <input id="fechaaprobacion" name="fechaaprobacion" type="datetime-local" class="form-control">
                            </div>
                        </div>

                        <div class="form-group form-float">
                            <label>Aprobado/Rechazado</label>
                            <select id="aprobadorechazado" name="aprobadorechazado" class="form-control">
                                <option value="APROBADO">APROBADO</option>
                                <option value="RECHAZADO">RECHAZADO</option>
                            </select>
                            <br>
                            <br>
                        </div>
                        <div class="form-group form-float">
                            <label>Aprobado por</label>
                            <select id="fkaprobador" name="fkaprobador" class="form-control show-tick">
                                {% for a in aprobador %}
                                    <option value="{{ a.id }}">{{ a.nombre ~ ' ' ~ a.apellido }}</option>
                                {% endfor %}
                            </select>
                            <br>
                            <br>
                        </div>

                        <div class="row clearfix">
                            <div class="col-sm-7">
                                <label class="form-label">Archivo digital</label>
                                <input id="archivo" name="archivo" type="file" class="file form-control validate" width="100" />
                                <br>
                                <br>
                            </div>
                        </div>
                    </div>

                    <div id="spn-nwrev" class="loader-wrapper" style="display: none">
                        <div class="loader loader-own">
                            <div class="preloader preloader-own">
                                <div class="spinner-layer pl-indigo">
                                    <div class="circle-clipper left"><div class="circle"></div></div>
                                    <div class="circle-clipper right"><div class="circle"></div></div>
                                </div>
                            </div>
                        </div>
                        <p class="text-center inf-own">Guardando...</p>
                    </div>
                        
                    <div class="modal-footer">
                        <button id="insert" type="submit" class="btn bg-indigo waves-effect">Guardar <i class="material-icons">save</i></button>
                        <button id="update" type="submit" class="btn bg-indigo waves-effect">Modificar <i class="material-icons">refresh</i></button>
                    </div>
                </div>
            </form>
        </div>

    </div>
</div>


